{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "import-graphql",
    [
      "babel-plugin-root-import",
      {
        "paths": [
          {
            "rootPathSuffix": "./src/",
            "rootPathPrefix": "@/"
          }
        ]
      }
    ],
    "@babel/plugin-proposal-class-properties",
    ["@babel/transform-runtime"]
  ]
}
